  "bugs": {
    "url": "https://github.com/RobertPurc/Trillo_project/issues"
  },
  "homepage": "https://github.com/RobertPurc/Trillo_project#readme",
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "node-sass": "^4.11.0",
    "postcss-cli": "^6.1.2"
  }
}